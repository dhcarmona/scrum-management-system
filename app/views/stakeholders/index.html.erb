<h1>Listing stakeholders</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Phone number</th>
      <th>Expectations</th>
      <th>Interest</th>
      <th>Power</th>
      <th>Project</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @stakeholders.each do |stakeholder| %>
      <tr>
        <td><%= stakeholder.name %></td>
        <td><%= stakeholder.email %></td>
        <td><%= stakeholder.phone_number %></td>
        <td><%= stakeholder.expectations %></td>
        <td><%= stakeholder.interest %></td>
        <td><%= stakeholder.power %></td>
        <td><%= stakeholder.project_id %></td>
        <td><%= link_to 'Show', stakeholder %></td>
        <td><%= link_to 'Edit', edit_stakeholder_path(stakeholder) %></td>
        <td><%= link_to 'Destroy', stakeholder, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Stakeholder', new_stakeholder_path %>
